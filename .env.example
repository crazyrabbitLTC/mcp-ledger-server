# Token/Blockchain Data Provider (Required)
DUNE_SIM_API_KEY=your_dune_sim_api_key_here

# Enhanced RPC Provider (Optional - improves performance)
ALCHEMY_API_KEY=your_alchemy_api_key_here

# Contract Verification (Optional - for ABI retrieval)
ETHERSCAN_API_KEY=your_etherscan_api_key_here

# Optional: Custom RPC Endpoints
MAINNET_RPC_URL=https://eth-mainnet.g.alchemy.com/v2/${ALCHEMY_API_KEY}
POLYGON_RPC_URL=https://polygon-mainnet.g.alchemy.com/v2/${ALCHEMY_API_KEY}
ARBITRUM_RPC_URL=https://arb-mainnet.g.alchemy.com/v2/${ALCHEMY_API_KEY}
OPTIMISM_RPC_URL=https://opt-mainnet.g.alchemy.com/v2/${ALCHEMY_API_KEY}
BASE_RPC_URL=https://base-mainnet.g.alchemy.com/v2/${ALCHEMY_API_KEY}

# Server Configuration
PORT=3000
NODE_ENV=development

# HTTP Authentication (Required for HTTP/Web deployment)
# For local STDIO usage, these are not needed
LEDGER_PROXY_API_KEY=your-secure-proxy-api-key-here
VERCEL_MCP_API_KEY=your-secure-mcp-api-key-here

# CORS Configuration (HTTP deployment only)
ALLOWED_ORIGINS=http://localhost:3000,https://your-frontend.vercel.app

# Proxy Service Configuration
LEDGER_PROXY_PORT=3001